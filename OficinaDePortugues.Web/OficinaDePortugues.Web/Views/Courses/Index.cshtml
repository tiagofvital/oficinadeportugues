                            @foreach (var course in Model.Courses)
                            {
                                <article id="lpr_course_post-@course.Id" class="lpr_course_post">
                                    <figure class="cmsmasters_img_rollover_wrap preloader"><img width="300" height="300" src="@course.ImageUrl" class="full-width wp-post-image" alt="@course.Name" title="@course.Name" srcset="http://language-school.cmsmasters.net/wp-content/uploads/2015/12/Depositphotos_63889211_original-300x300.jpg 300w, http://language-school.cmsmasters.net/wp-content/uploads/2015/12/Depositphotos_63889211_original-150x150.jpg 150w, http://language-school.cmsmasters.net/wp-content/uploads/2015/12/Depositphotos_63889211_original-60x60.jpg 60w, http://language-school.cmsmasters.net/wp-content/uploads/2015/12/Depositphotos_63889211_original-580x580.jpg 580w" sizes="(max-width: 300px) 100vw, 300px" /><div class="cmsmasters_img_rollover"><a href='@Url.Action("Index","CourseDetail", new { permalink = course.Permalink })' title="@course.Name" class="cmsmasters_open_post_link"></a></div></figure>

                                    <div class="lpr_course_inner">
                                        <header class="entry-header lpr_course_header">
                                            <h6 class="entry-title lpr_course_title"><a href='@Url.Action("Index","CourseDetail", new { permalink = course.Permalink })'>@course.Name</a></h6>
                                        </header>
                                        <div class="review-stars-rated">
                                            <div class="review-stars empty"></div>

                                            <div class="review-stars filled" style="width:@course.RatingWidth;"></div>
                                        </div><span class="cmsmasters_course_price">@course.Price</span>
                                    </div>
                                    <footer class="cmsmasters_course_footer">
                                        <div class="cmsmasters_cource_duration">@course.Duration</div>
                                        <div class="entry-meta cmsmasters_cource_cat">
                                            @foreach (var cat in course.Categories)
                                            {
                                                <a>@cat</a>

                                                if (course.Categories.Last() != cat)
                                                {
                                                    @Html.Raw(",");
                                                }
                                            }
                                        </div>
                                    </footer>
                                </article>
                            }
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
